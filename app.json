{
  "name": "El GÃ©o Paso - DEV",
  "description": "A simple web application to display charts about French jobs offers in GIS.",
  "image": "heroku/python",
  "repository": "https://github.com/guts/elgeopaso/",
  "website": "https://elgeopaso.georezo.net/",
  "keywords": [
    "python",
    "django",
    "GIS",
    "georezo",
    "job offers"
  ],
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev",
      "options": {
        "version": "12"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DJANGO_ALLOWED_HOSTS": {
      "description": "Host/domain names that this Django site can serve",
      "value": ".herokuapp.com"
    },
    "DJANGO_DEBUG": {
      "description": "Debug or not debug",
      "value": "0"
    },
    "DJANGO_SECRET_KEY": {
      "description": "The secret key for the Django application.",
      "generator": "secret"
    },
    "WEB_CONCURRENCY": {
      "description": "The number of processes to run.",
      "value": "4"
    }
  },
  "environments": {
    "review": {
      "env": {
        "DJANGO_ALLOWED_HOSTS": {
          "description": "Host/domain names that this Django site can serve",
          "value": ".herokuapp.com"
        },
        "DJANGO_DEBUG": {
          "description": "Debug or not debug",
          "value": "1"
        },
        "DJANGO_SECRET_KEY": {
          "description": "The secret key for the Django application.",
          "generator": "secret"
        },
        "WEB_CONCURRENCY": {
          "description": "Number of processes to run.",
          "value": "4"
        }
      },
      "addons": {
        "plan": "heroku-postgresql:hobby-dev",
        "options": {
          "version": "12"
        }
      }
    }
  }
}